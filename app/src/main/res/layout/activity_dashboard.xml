<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nestedScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.DashboardActivity">


    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!--Toolbar-->
        <include layout="@layout/toolbar" />
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="1"
            android:layout_marginTop="20dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:orientation="vertical">

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">
                <android.support.v7.widget.CardView
                    android:id="@+id/opregistration"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    android:layout_weight="1"
                    app:cardUseCompatPadding="false" >

                    <android.support.v7.widget.LinearLayoutCompat
                        android:id="@+id/transfermoney"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/op"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            />
                        <android.support.v7.widget.AppCompatTextView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/op_registration"
                            android:textColor="#01073D"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:paddingStart="6dp"
                            android:paddingEnd="6dp"
                            android:textSize="14sp" />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/medicalhistory"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    app:cardUseCompatPadding="false" >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:id="@+id/cashout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/medical"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            />
                        <android.support.v7.widget.AppCompatTextView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/medical_history"
                            android:textColor="#01073D"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:paddingStart="6dp"
                            android:paddingEnd="6dp"
                            android:textSize="14sp" />

                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.CardView>

            </android.support.v7.widget.LinearLayoutCompat>


            <android.support.v7.widget.LinearLayoutCompat
                android:id="@+id/addfunds"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">
                <android.support.v7.widget.CardView
                    android:id="@+id/chiefcomplaints"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    app:cardUseCompatPadding="false" >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:id="@+id/walletbalance"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/chief"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            />
                        <android.support.v7.widget.AppCompatTextView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/chief_complaints"
                            android:textColor="#01073D"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:paddingStart="6dp"
                            android:paddingEnd="6dp"
                            android:textSize="14sp" />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/diagnosticaids"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    app:cardUseCompatPadding="false" >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/diagnostic"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            />
                        <android.support.v7.widget.AppCompatTextView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/diagnostic_aids_investigations"
                            android:textColor="#01073D"
                            android:layout_weight="1"
                            android:paddingStart="6dp"
                            android:paddingEnd="6dp"
                            android:gravity="center"
                            android:textSize="14sp" />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.CardView>


            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">
            <android.support.v7.widget.CardView
                android:id="@+id/treatmentplans"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:layout_margin="5dp"
                android:layout_weight="1"
                app:cardCornerRadius="4dp"
                app:cardElevation="4dp"
                app:cardUseCompatPadding="false" >
                <android.support.v7.widget.LinearLayoutCompat
                    android:id="@+id/ministatement"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <android.support.v7.widget.AppCompatImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/treatment"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        />
                    <android.support.v7.widget.AppCompatTextView

                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/treatment_plans_medications"
                        android:textColor="#01073D"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingStart="6dp"
                        android:paddingEnd="6dp"
                        android:textSize="14sp" />
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/surgicalprocedure"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:layout_margin="5dp"
                android:layout_weight="1"
                app:cardCornerRadius="4dp"
                app:cardElevation="4dp"
                app:cardUseCompatPadding="false" >
                <android.support.v7.widget.LinearLayoutCompat
                    android:id="@+id/help"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <android.support.v7.widget.AppCompatImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/surgical"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        />
                    <android.support.v7.widget.AppCompatTextView

                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/surgical_procedure_intervention"
                        android:textColor="#01073D"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingStart="6dp"
                        android:paddingEnd="6dp"
                        android:textSize="14sp" />
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.CardView>
            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">
                <android.support.v7.widget.CardView
                    android:id="@+id/settings"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    app:cardUseCompatPadding="false" >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/settings"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            />
                        <android.support.v7.widget.AppCompatTextView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/settings"
                            android:textColor="#01073D"
                            android:layout_weight="1"
                            android:paddingStart="6dp"
                            android:paddingEnd="6dp"
                            android:gravity="center"
                            android:textSize="14sp" />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/reports"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    app:cardUseCompatPadding="false" >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/reports"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            />
                        <android.support.v7.widget.AppCompatTextView

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/reports"
                            android:textColor="#01073D"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:paddingStart="6dp"
                            android:paddingEnd="6dp"
                            android:textSize="14sp" />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.CardView>
            </android.support.v7.widget.LinearLayoutCompat>


        </android.support.v7.widget.LinearLayoutCompat>
    </android.support.v7.widget.LinearLayoutCompat>
</android.support.v4.widget.NestedScrollView>